    "url": "git+https://github.com/m7medvision/lazycommit.git"
  },
  "main": "dist/index.js",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "latest",
    "bumpp": "^9.7.1",
